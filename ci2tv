#!/usr/bin/env bash

# Read a chartink (https://chartink.com) generated file and convert it to
# tradingview readable file.

if [ $# -ne 1 ]; then
    echo "Usage: $0 FILE"
    exit 1
fi

ci_file="$1"

if [[ ! -f $ci_file ]]; then
    echo "File $ci_file not found"
    exit 1
fi

awk -F, '{ print $3 }' "$ci_file" |
    awk -F\" '{ print "NSE:"$2 }' |
    sed 1d



